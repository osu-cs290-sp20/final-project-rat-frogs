<article class="frog">
	<div class="frog-image">
		<img class="frog-img" src={{url}}>
		<p class="frog-caption">
			{{caption}}
		</p>
	</div>
	<div class="frog-container-content">
		<p class="frog-name">
			{{name}}
		</p>
		<p class="frog-diet">
			{{diet}}
		</p>
		<p class="frog-misc">
			{{misc}}
		</p>
	</div>
</article>