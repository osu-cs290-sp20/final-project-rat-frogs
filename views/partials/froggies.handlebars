<article class="frog">
		<div class="frog-image">
			<img class="frog-img" src={{url}}>
			<p class="frog-caption">
				{{frogCaption}}
			</p>
		</div>
	<div class="frog-container-content">
		<p class="frog-name">
			{{frogName}}
		</p>
		<p class="frog-diet">
			{{frogDiet}}
		</p>
		<p class="frog-misc">
			{{frogMisc}}
		</p>
	</div>
</article>